                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface {#IFDESCR}: Operational status</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.14331.5.5.1.5.1.9.{#SNMPINDEX}</snmp_oid>
                            <key>net.topsec.if.status[ifOperStatus.{#SNMPINDEX}]</key>
                            <history>7d</history>
                            <trends>0</trends>
                            <applications>
                                <application>
                                    <name>Network interfaces</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>SNMP interface status (ifOperStatus)</name>
                            </valuemap>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{$IFCONTROL:&quot;{#IFDESCR}&quot;}=1 and {last()}=2 and {diff()}=1</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{last()}&lt;&gt;2</recovery_expression>
                                    <name>Interface {#IFDESCR}: Link down</name>
                                    <opdata>Current state: {ITEM.LASTVALUE1}</opdata>
                                    <priority>AVERAGE</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{Template Net TOPSEC SNMPv2:net.topsec.if.speed[ifHighSpeed.{#SNMPINDEX}].change()}&lt;0 and {Template Net TOPSEC SNMPv2:net.topsec.if.speed[ifHighSpeed.{#SNMPINDEX}].last()}&gt;0 and {Template Net TOPSEC SNMPv2:net.topsec.if.status[ifOperStatus.{#SNMPINDEX}].last()}&lt;&gt;2</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{Template Net TOPSEC SNMPv2:net.topsec.if.speed[ifHighSpeed.{#SNMPINDEX}].change()}&gt;0 and {Template Net TOPSEC SNMPv2:net.topsec.if.speed[ifHighSpeed.{#SNMPINDEX}].prev()}&gt;0 and {Template Net TOPSEC SNMPv2:net.topsec.if.status[ifOperStatus.{#SNMPINDEX}].last()}=2</recovery_expression>
                            <name>Interface {#IFDESCR}: Ethernet has changed to lower speed than it was before</name>
                            <opdata>Current reported speed: {ITEM.LASTVALUE1}</opdata>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                            <dependencies>
                                <dependency>
                                    <name>Interface {#IFDESCR}: Link down</name>
                                    <expression>{$IFCONTROL:&quot;{#IFDESCR}&quot;}=1 and {Template Net TOPSEC SNMPv2:net.topsec.if.status[ifOperStatus.{#SNMPINDEX}].last()}=2 and {Template Net TOPSEC SNMPv2:net.topsec.if.status[ifOperStatus.{#SNMPINDEX}].diff()}=1</expression>
                                    <recovery_expression>{Template Net TOPSEC SNMPv2:net.topsec.if.status[ifOperStatus.{#SNMPINDEX}].last()}&lt;&gt;2</recovery_expression>
                                </dependency>
                            </dependencies>
                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>{Template Net TOPSEC SNMPv2:net.topsec.if.in.errors[ifInErrors.{#SNMPINDEX}].min(5m)}&gt;{$IF.ERRORS.WARN:&quot;{#IFDESCR}&quot;} or {Template Net TOPSEC SNMPv2:net.topsec.if.out.errors[ifOutErrors.{#SNMPINDEX}].min(5m)}&gt;{$IF.ERRORS.WARN:&quot;{#IFDESCR}&quot;}</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{Template Net TOPSEC SNMPv2:net.topsec.if.in.errors[ifInErrors.{#SNMPINDEX}].max(5m)}&lt;{$IF.ERRORS.WARN:&quot;{#IFDESCR}&quot;}*0.8 and&#13;
{Template Net TOPSEC SNMPv2:net.topsec.if.out.errors[ifOutErrors.{#SNMPINDEX}].max(5m)}&lt;{$IF.ERRORS.WARN:&quot;{#IFDESCR}&quot;}*0.8</recovery_expression>
                            <name>Interface {#IFDESCR}: High error rate ( &gt; {$IF.ERRORS.WARN:&quot;{#IFDESCR}&quot;} for 5m)</name>
                            <opdata>errors in: {ITEM.LASTVALUE1}, errors out: {ITEM.LASTVALUE2}</opdata>
                            <priority>WARNING</priority>
                            <description>Recovers when below 80% of {$IF.ERRORS.WARN:&quot;{#IFDESCR}&quot;} threshold</description>
                            <manual_close>YES</manual_close>
                            <dependencies>
                                <dependency>
                                    <name>Interface {#IFDESCR}: Link down</name>
                                    <expression>{$IFCONTROL:&quot;{#IFDESCR}&quot;}=1 and {Template Net TOPSEC SNMPv2:net.topsec.if.status[ifOperStatus.{#SNMPINDEX}].last()}=2 and {Template Net TOPSEC SNMPv2:net.topsec.if.status[ifOperStatus.{#SNMPINDEX}].diff()}=1</expression>
                                    <recovery_expression>{Template Net TOPSEC SNMPv2:net.topsec.if.status[ifOperStatus.{#SNMPINDEX}].last()}&lt;&gt;2</recovery_expression>
                                </dependency>
                            </dependencies>
                        </trigger_prototype>
